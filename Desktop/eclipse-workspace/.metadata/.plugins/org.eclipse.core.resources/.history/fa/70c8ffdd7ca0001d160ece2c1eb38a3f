package edu.whileex;

public class WhileEx0130 {

	public static void main(String[] args) {
		int i =0;
		while (i <= 7)
		{
			System.out.printf("i=%d\n", i);
			++i;
		}
//		while(true)
//			System.out.println("while 무한반");
		int k =0 ;
		do {
			System.out.printf("k=%d\n", k);
			++k;
		} while (k<=7);  // do 블럭안의 명령문이 최소 한번은 실행됨
	}

}
